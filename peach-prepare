#!/bin/sh

# first create the staging directory

if [ -d "staging" ]; then
    rm -r staging/*
else
    mkdir staging
fi

# do some ugly sed scripts to make everything peach-ready

packages=( algorithm model )

for package in ${packages[@]}
do
    for f in $package/*.java
    do
        cat $f | sed -f peach-prepare.sed > staging/$(basename $f)
    done
done

cat Main.java | sed -f peach-prepare.sed > staging/Main.java

#sed -e peach-prepare.sed algorithm/Algorithm.java


